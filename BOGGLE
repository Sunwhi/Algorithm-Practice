bool keepSearching(vector<vector<char>> &array, int find, int x, int y, wordSize){
	if(find == 0) return false;
	if(find == wordSize) return true;
		
	if(search8Direction(array,word[find],i,j))
		keepSearching(array,find+1,*newx,*newy);
	else
		keepSearching(array,find-1,*newx,*newy);
}
vector<char> find_word(vector<vector<char>> &array, vector<char> &word){
	int wordSize = word.size();
